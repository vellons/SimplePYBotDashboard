{
  "name": "simplepybotdashboard",
  "author": "vellons",
  "description": "Basic development dashboard for SimplePYBotSDK",
  "version": "0.7.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:dev": "vue-cli-service build && electron .",
    "electron:build": "vue-cli-service build --mode production && electron-builder build"
  },
  "dependencies": {
    "@meforma/vue-toaster": "^1.2.2",
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "vue": "^3.0.0",
    "vue-axios": "^3.2.4",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/reactivity": "^3.0.7",
    "babel-eslint": "^10.1.0",
    "electron": "^21.2.2",
    "electron-builder": "^23.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0"
  },
  "main": "src/electron_main.js",
  "build": {
    "appId": "0.7.1",
    "win": {
      "target": "nsis"
    },
    "files": ["**/*", "dist/**/*"],
    "directories": {
      "buildResources": "dist",
      "output": "dist_release_electron"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
